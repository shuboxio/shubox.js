{
  "name": "shubox",
  "version": "1.1.0",
  "description": "Shubox javascript SDK",
  "author": "team@shubox.io",
  "license": "MIT",
  "keywords": [],
  "main": "./dist/shubox.umd.js",
  "module": "./dist/shubox.es.js",
  "typings": "./dist/shubox.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npx lefthook install",
    "test-debugger": "vitest --inspect-brk --pool forks --poolOptions.forks.singleFork",
    "test-watch": "vitest",
    "test": "vitest run",
    "dev": "vite --port 8888",
    "build": "vitest run && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --ext .ts,.js",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,html}\""
  },
  "dependencies": {
    "dropzone": "5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@playwright/test": "^1.56.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/dropzone": "^5.7.8",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "^25.0.0",
    "lefthook": "^2.0.2",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  }
}
